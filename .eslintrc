{
  "parser": "@babel/eslint-parser",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "linebreak-style": "off",
    "object-curly-spacing": "off",
    "require-jsdoc": "off",
    "operator-linebreak": "off",
    "no-debugger": "off",
    "indent": "off",
    "comma-dangle": "off",
    "space-before-function-paren": "off",
    "no-invalid-this": "off"
  },
  "extends": [
    "eslint:recommended",
    "google"
  ]
}